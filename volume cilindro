#include<stdio.h>

int main()
{   
    float raio, altura, volume;
    const float pi = 3.14;

    printf("Digite o raio do cilindro: ");
    scanf("%f",&raio);
    printf("Digite a altura do cilindro: ");
    scanf("%f",&altura);
    volume = pi * raio * raio * altura;
    printf("O volume do cilindro e: %f\n", volume);
    return 0;

}
