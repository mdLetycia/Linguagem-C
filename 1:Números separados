#include<stdio.h>

int main()
{   
    int i, n1,n2,n3,n4,n5;
    
    printf("digite um número de 5 digdígitos: ");
    scanf("%d",&i);
    
    n1=i/10000;
    n2=(i/1000)%10;
    n3=(i/100)% 10;
    n4=(i/10)% 10;
    n5=(i%10);
    
    
    
    printf("%d %d %d %d %d",n1,n2,n3,n4,n5);
    
    
    return 0;

}
