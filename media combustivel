#include<stdio.h>

int main()
{   
    float v1,v2,v3,media;

    printf("Digite o primeiro valor: ");
    scanf("%f", &v1);
    printf("Digite o segundo valor: ");
    scanf("%f", &v2);
    printf("Digite o terceiro valor: ");
    scanf("%f", &v3);
    media = (v1 + v2 + v3) / 3;
    
    printf("A média é: %f\n", media);

    return 0;

}
