
#include <stdio.h>

int main() {
    int num;

    printf("Digite um numero inteiro: ");
    scanf("%d", &num);

    if ((num % 2 == 0)) {
        printf("O numero %d e divisivel por 2.\n", num);
    } else if((num % 3 == 0)){
    printf("O numero %d e divisivel por 3.\n", num);
    }else{ printf("O numero %d nao corresponde a condição.\n", num);}
    
    return 0;
}
