#include <stdio.h>

int main() {
  //Declaração de variavel
  float a,b,c;
  
  //Entrada de dados
  printf("Informe a medida do segmento a: ");
  scanf("%f",&a);
  printf("Informe a medida do segmento b: ");
  scanf("%f",&b);
  printf("Informe a medida do segmento c ");
  scanf("%f",&c);
  
  //processamento
  if(a<b+c,b<a+c,c<a+b){
      printf("É possivel formar um triangulo");
  }else{
      printf("não é possivel formar um triangulo");
  }
  

    return 0;
}
